/**app.wxss**/
@import '../../model/model.wxss';
page{  
  position:fixed;
  background-color: white;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index:0;
  width: 100%;
  height: 100%;
  font-size: 28rpx;
}


/*panel & title*/
.panel{
  border-top:2rpx solid #e1d6cf; 
  border-bottom:2rpx solid #e1d6cf;
  background-color: #fff;
  padding: 20rpx;
}
.panel-title{
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.panel-title-txt{
  display: inline-block;
  font-size: 36rpx;
  color:#8a694e;
}
.panel-title-icon{
  display: inline-block;
  font-size: 40rpx;
  color:#c4b4a6;
}
/*section*/
.section{
  margin: 20rpx;
  padding: 20rpx;
}
/*图片*/
image{width: 100%;height:auto;}
.ellipsis{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
/*阴影层*/
.shadow{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.8;
  top:0;
  left: 0;
  z-index: -1;
}
.navigator-hover{
  color: #664e3a;
}
/*技术支持图标*/
.copyright{
  position: fixed;
  z-index:-1;
  bottom:20rpx;
  width:100%;
  text-align:center;
}
.copyright image{
  width: 30%;
}
/*图标*/
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-xiayiye:before { content: "\e636";}
.icon-wifibicon:before { content: "\e633";}
.icon-tingchewei:before { content: "\e638";}
.icon-wuyanfang:before { content: "\e677";}
.icon-baoxiang:before { content: "\e603";}
.icon-plus:before { content: "\e662";}
.icon-gouwuchekong:before { content: "\e600";}
.icon-del:before { content: "\e616";}
.icon-daifukuan:before { content: "\e64b"; }
.icon-yiwancheng:before { content: "\e7d7"; }



/*********************************/
.view_bottom{
  margin-top: 20rpx;
  height: 30rpx;
  display: flex;
  justify-content: center;
  flex-direction: row;
}

.view_bottom text{
  font-size: small;
  text-decoration:underline;
  color: #2A76BC;
  margin-left: 10rpx;
}

.swiperstyle{
  display: flex;
  flex-direction: column;
  height:100vh;
  background:rgb(240, 238, 254);
}
.swiperstyleHide{
  position: fixed;
  bottom: -100%rpx;
  height: 0;  
  width: 100%;  
  overflow: hidden
}